{
	"servers": {
		"next-devtools": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"next-devtools-mcp@latest"
			]
		},
		"shadcn": {
			"command": "npx",
			"args": [
				"shadcn@latest",
				"mcp"
			]
		},
		"ai-elements": {
			"command": "npx",
			"args": [
				"-y",
				"mcp-remote",
				"https://registry.ai-sdk.dev/api/mcp"
			]
		}
	},
	"inputs": []
}